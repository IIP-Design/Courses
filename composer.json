{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.5.3",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/4.5.3.zip"
                },
                "require" : {
                    "fancyguy/webroot-installer": "1.1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "git",
            "url": "git@github.com:USStateDept/wp-hide-login-form.git"
        },
        {

            "type": "git",
            "url": "git@github.com:IIP-Design/courses-custom-posts.git"
        },
        {
            "type": "git",
            "url": "git@github.com:IIP-Design/corona.git"
        }
    ],
    "require": {
        "wordpress": "4.5.3",
        "fancyguy/webroot-installer": "1.1.0",
        "koodimonni/composer-dropin-installer": "1.0",
        "usstatedept/wp-hide-login-form": "1.0.1",
        "iip-design/courses-custom-posts": "1.0.0",
        "iip-design/corona": "2.7.1",
        "wpackagist-plugin/ewww-image-optimizer": "2.9.3",
        "wpackagist-plugin/google-apps-login": "2.9.7",
        "wpackagist-plugin/wordpress-mu-domain-mapping": "0.5.5.1"
        },
    "extra": {
        "dropin-paths": {
            "wp-content/": [
              "package:wpackagist-plugin/wordpress-mu-domain-mapping:sunrise.php"
            ]
        },
        "webroot-dir": "wp",
        "webroot-package": "wordpress"
    }
}
